<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddActivity">

    <ImageButton
        android:id="@+id/garage"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:srcCompat="@drawable/room_11"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/kidRoom"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView20"
        app:layout_constraintStart_toStartOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:srcCompat="@drawable/room_13"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/toilet"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView19"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        app:srcCompat="@drawable/room_12"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textViewFon"
        android:layout_width="415dp"
        android:layout_height="50dp"
        android:background="#282142"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/titleKitchen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Room"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/textViewFon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textViewFon"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:backgroundTint="#282142"
        android:text="SAV"
        app:layout_constraintBottom_toBottomOf="@+id/textViewFon"
        app:layout_constraintEnd_toEndOf="@+id/textViewFon"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/titleKitchen"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/back2"
        android:layout_width="42dp"
        android:layout_height="50dp"
        android:background="#282142"
        android:onClick="startNewActivity"
        app:layout_constraintBottom_toBottomOf="@+id/textViewFon"
        app:layout_constraintEnd_toStartOf="@+id/titleKitchen"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/chevron_left"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Enter Room's name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewFon" />

    <EditText
        android:id="@+id/edRoom"
        android:layout_width="374dp"
        android:layout_height="48dp"
        android:backgroundTint="#7F7F7F"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Select room's"
        app:layout_constraintEnd_toEndOf="@+id/edRoom"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edRoom"
        app:layout_constraintTop_toBottomOf="@+id/edRoom" />

    <ImageButton
        android:id="@+id/kitchen"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/frame1_3"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/bedroom"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toEndOf="@+id/kitchen"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/room_5"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/bathroom"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bedroom"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/room_6"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/office"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:srcCompat="@drawable/room_7"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/livingRoom"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:srcCompat="@drawable/room_10"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/tvRoom"
        android:layout_width="113dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView16"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:srcCompat="@drawable/room_8"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kitchen"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/kitchen"
        app:layout_constraintStart_toStartOf="@+id/kitchen"
        app:layout_constraintTop_toBottomOf="@+id/kitchen" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="Bedroom"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/bedroom"
        app:layout_constraintStart_toStartOf="@+id/bedroom"
        app:layout_constraintTop_toBottomOf="@+id/bedroom" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bathroom"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/bathroom"
        app:layout_constraintStart_toStartOf="@+id/bathroom"
        app:layout_constraintTop_toBottomOf="@+id/bathroom" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Office"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/office"
        app:layout_constraintStart_toStartOf="@+id/office"
        app:layout_constraintTop_toBottomOf="@+id/office" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TV Room"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tvRoom"
        app:layout_constraintStart_toStartOf="@+id/tvRoom"
        app:layout_constraintTop_toBottomOf="@+id/tvRoom" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Living Room"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/livingRoom"
        app:layout_constraintStart_toStartOf="@+id/livingRoom"
        app:layout_constraintTop_toBottomOf="@+id/livingRoom" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Garage"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/garage"
        app:layout_constraintStart_toStartOf="@+id/garage"
        app:layout_constraintTop_toBottomOf="@+id/garage" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kid Room"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/kidRoom"
        app:layout_constraintStart_toStartOf="@+id/kidRoom"
        app:layout_constraintTop_toBottomOf="@+id/kidRoom" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Toilet"
        android:textColor="#040404"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/toilet"
        app:layout_constraintStart_toStartOf="@+id/toilet"
        app:layout_constraintTop_toBottomOf="@+id/toilet" />

</androidx.constraintlayout.widget.ConstraintLayout>